  <div class="title">
      <h1>Books</h1>
  </div>
  <div class="table">
    <table border="1">
      <tr>
        <th>Title</th>
        <th>Body</th>
      </tr>
      
      <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
      </tr>
      <% end %>
    </table>
  </div>